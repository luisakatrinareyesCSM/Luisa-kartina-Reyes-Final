@page
@model Kat.Pages.Manage.Classes.Delete
@{
}

<div class="row">
    <div class="col-4">
        <div class="card">
            <div class="card-body">
                <form method="Post">
                    <div class="form-group">
                        <label asp-for="View.Code">Code</label>
                        <input type="text" class="form-control" placeholder="Enter role name" asp-for="View.Code">
                    </div>
                    <div class="form-group">
                        <label asp-for="View.YearLevel">Year Level</label>
                        <input type="text" class="form-control" placeholder="Enter FullName" asp-for="View.YearLevel">
                    </div>
                    <div class="form-group">
                        <label asp-for="View.StartDate">Start Date</label>
                        <input type="date" class="form-control" placeholder="Enter  " asp-for="View.StartDate">
                    </div>
                    <div class="form-group">
                        <label asp-for="View.Meeting">meeting</label>
                        <select class="form-control" id="Sex" asp-for="View.Meeting">
                            <option value="0">MMF</option>
                            <option value="1">TTH</option>
                            <option value="2">SAT</option>

                        </select>
                    </div>
                    <div class="form-group alert-danger">
                        @Html.ValidationSummary()
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-primary">Delete</button>
                        <a href="~/manage/Classes" class="btn btn-secondary">Cancel</a>
                    </div>


                </form>
            </div>
        </div>
    </div>
</div>
